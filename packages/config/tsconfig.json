{
  "extends": "../../tsconfig.base.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/index.ts",
    "src/tamagui.config.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ESNext",
    "outDir": "./dist",
    "rootDir": "src",
    "declaration": true,
    "emitDeclarationOnly": false,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "references": []
}
