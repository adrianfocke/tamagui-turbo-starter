{
  "name": "@bbook/config",
  "version": "0.0.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "type": "module",
  "types": "./src",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
