{
  "name": "turbobook",
  "type": "module",
  "version": "1.0.0",
  "description": "Monorepo for Bentobook",
  "main": "index.js",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev-web": "pnpm --filter ./apps/web dev",
    "dev-ios": "pnpm --filter ./apps/mobile start -- --ios",
    "run-ios": "pnpm --filter ./apps/mobile exec expo run:ios",
    "run-android": "pnpm --filter ./apps/mobile exec expo run:android",
    "prepare": "husky",
    "generate:i18n": "pnpm --filter @bbook/i18n generate:resources"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.1",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "eslint-config-expo": "^9.0.2",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0"
  }
}
